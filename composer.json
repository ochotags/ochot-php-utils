{
    "name": "ochotags/ochot-php-utils",
    "type": "library",
    "description": "PHP Libraries",
    "keywords": ["php", "library", "utils"],
    "homepage": "https://github.com/ochotags/ochot-php-utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Corredor",
            "email": "macorredor@8tags.es"
        }
    ],
    "require": {
        "php": ">=7.1",
        "phpmailer/phpmailer": "^6.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-mysqli": "*"
    },
    "require-dev": {
        "vimeo/psalm": "^4.7",
        "phpstan/phpstan": "^0.12.88",
        "phpmd/phpmd": "^2.10",
        "phpunit/phpunit": "^9.5",
        "phpunit/php-code-coverage": "^9.2",
        "squizlabs/php_codesniffer": "^3.6",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "OchoPhpUtils\\": "src/",
            "tests\\": "tests"
        }
    }
}